<% if ENV['JEKYLL_ENV'] == 'staging' %>baseurl: /screeningone<% end %>
title: ScreeningOne
markdown: kramdown
highlighter: null
exclude:
  - Gemfile
  - Gemfile.lock
  - bin
  - README.md
  - .env
  - .*.env
  - s3_website.yml
  - lambda

assets:
  dirname: assets
  sources:
    - _assets
  debug: false
  js_compressor: uglifier
  css_compressor: sass

contentful:
  spaces:
    - proposal:
        space: lxg5s6vl1fqd
        access_token: <%= ENV['CONTENTFUL_ACCESS_TOKEN'] %>
        content_types:
          proposal: SlugObfuscator

page_gen-dirs: true
page_gen:
  - data: contentful.spaces.proposal.proposal
    template: proposal
    name: title
    dir: proposal
