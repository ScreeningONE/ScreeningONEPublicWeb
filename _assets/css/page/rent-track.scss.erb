.page.rent-track {
  padding: 30px 0 0;

  .form-control {
    -webkit-box-shadow: 0 0 0;
    box-shadow: 0 0 0;
    border-radius: 0;
  }

  .btn {
    border-radius: 0;
    font-size: 16px;
    padding: 20px 0;
  }
  
  h1.title {
    font-size: 40px;
    margin: 0 0 15px;
  }

  .desc {
    p {
      font-size: 20px;
    }
  }

  hr {
    margin: 30px 0;
  }

  h2 {
    font-size: 30px;
    margin: 0 0 15px;
  }
}